<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">
        
        
    <context:annotation-config/> 
 
	<bean id="student" class="com.zzy.beanAnnotationConfig.Student">
		<property name="name" value="zhou"> </property> 
		<property name="age" value="23"> </property> 
	</bean> 
	
	<bean id="addr" class="com.zzy.beanAnnotationConfig.Address">
		<property name="addr" value="浦东新区"></property>
	</bean>
	
	<bean id="addr02" class="com.zzy.beanAnnotationConfig.Address">
		<property name="addr" value="嘉定区"></property>
	</bean>
	

</beans>