<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">
 
	<bean id="collectionsInjection" class="com.zzy.beanCollectionsInjection.CollectionsInjectionClazz">
		<property name="list">
			<list>
				<value>zhou</value>
				<value>lee</value>
				<value>lee</value>
			</list>
		</property>
		
		<property name="set">
			<set>
				<value>zhou</value>
				<value>lee</value>
				<value>lee</value>
			</set>
		</property>
		
		<property name="map">
			<map>
				<entry key="name" value="zhou"></entry>
				<entry key="age" value="23"></entry>
				<entry key="englishName" value="john" />
			</map>
		</property>
		
		<property name="props">
			<props>
				<prop key="name">zzy</prop>
				<prop key="age">keep secret</prop>
				<prop key="gender">male</prop>
			</props>
		</property>
	</bean> 
	
	
</beans>